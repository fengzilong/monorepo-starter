{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "dependsOn": [
        "^dev"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "coverage": {
      "dependsOn": [
        "^coverage"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    }
  }
}