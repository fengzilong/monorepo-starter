{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": [
      "ES2018",
      "DOM",
      "DOM.Iterable"
    ],
    "target": "ES2018",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "outDir": "dist",
    "allowJs": false
  }
}